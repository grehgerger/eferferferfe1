getgenv().resolver = true
local Aiming = loadstring(game:HttpGet("https://raw.githubusercontent.com/grehgerger/erferfefefer2/main/xddd", true))()
Aiming.TeamCheck(false)

local Workspace = game:GetService("Workspace")
_G.resolver = loadstring(game:HttpGet("https://https://raw.githubusercontent.com/grehgerger/eereerreerfresolv/main/res", true))()
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local LocalPlayer = Players.LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local CurrentCamera = Workspace.CurrentCamera

local lemonware = {
    SilentAim = true,
    AimLock = false,
    Prediction = 0.14,
    AimLockKeybind = Enum.KeyCode.E
}
getgenv().lemonware = lemonware

function Aiming.Check()
    if not (Aiming.Enabled == true and Aiming.Selected ~= LocalPlayer and Aiming.SelectedPart ~= nil) then
        return false
    end
    local Character = Aiming.Character(Aiming.Selected)
    local KOd = Character:WaitForChild("BodyEffects")["K.O"].Value
    local Grabbed = Character:FindFirstChild("GRABBING_CONSTRAINT") ~= nil
    if (KOd or Grabbed) then
        return false
    end
    return true
end
local __index
__index =
    hookmetamethod(
    game,
    "__index",
    function(t, k)
        if (t:IsA("Mouse") and (k == "Hit" or k == "Target") and Aiming.Check()) then
            local SelectedPart = Aiming.SelectedPart
            if (lemonware.SilentAim and (k == "Hit" or k == "Target")) then
                local Hit = SelectedPart.CFrame + (SelectedPart.Velocity * lemonware.Prediction)
                return (k == "Hit" and Hit or SelectedPart)
            end
        end

        return __index(t, k)
    end
)
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

mouse.KeyDown:Connect(
    function(key)
        if key == "k" then
            if lemonware.SilentAim == false then
                lemonware.SilentAim = true
            else
                lemonware.SilentAim = false
            end
        end
    end
)
RunService:BindToRenderStep(
    "AimLock",
    0,
    function()
        if (lemonware.AimLock and Aiming.Check() and UserInputService:IsKeyDown(lemonware.AimLockKeybind)) then
            local SelectedPart = Aiming.SelectedPart

            local Hit = SelectedPart.CFrame + (SelectedPart.Velocity * lemonware.Prediction)

            CurrentCamera.CFrame = CFrame.lookAt(CurrentCamera.CFrame.Position, Hit.Position)
        end
    end
)
